{
  "name": "@tern-secure/react",
  "version": "1.1.9",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TernSecure/auth.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TernSecure/auth/issues"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup",
    "build:add": "tsc -p tsconfig.add.json",
    "dev": "tsup --watch",
    "lint": "eslint src"
  },
  "dependencies": {
    "@tern-secure/shared": "workspace:*",
    "@tern-secure/types": "workspace:*",
    "tslib": "catalog:repo"
  },
  "devDependencies": {
    "next": "^15.0.0"
  },
  "peerDependencies": {
    "firebase": "catalog:peer-firebase",
    "react": "catalog:peer-react",
    "react-dom": "catalog:peer-react"
  },
  "publishConfig": {
    "access": "public"
  }
}
